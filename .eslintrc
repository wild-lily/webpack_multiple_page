{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "semi-style": ["error", "last"],
    "space-before-function-paren": [2, {"anonymous": "always", "named": "never"}],
    "comma-dangle": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "arrow-parens": 0
  },
  "plugins": ["react"],
  "extends": "airbnb"
}